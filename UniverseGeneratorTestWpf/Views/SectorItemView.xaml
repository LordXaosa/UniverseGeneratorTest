             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UniverseGeneratorTestWpf.Views"
             xmlns:conv="clr-namespace:UniverseGeneratorTestWpf.Converters"
             mc:Ignorable="d"
             Width="{StaticResource Multiplier}"
             Height="{StaticResource Multiplier}">
    <Control.Style>
        <Style TargetType="Control">
            <Setter Property="Template" Value="{StaticResource mainTempl}"/>
        </Style>
    </Control.Style>
